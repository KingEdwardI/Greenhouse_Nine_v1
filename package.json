{
  "name": "@highwind/greenhouse-nine",
  "description": "Core design system for the Highwind project",
  "version": "0.0.0",
  "type": "module",
  "author": "Edward Vetter-Drake",
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix",
    "ladle": "ladle serve",
    "generate:css": "tsx scripts/generateRadixCSS.ts",
    "preladle": "pnpm run generate:css",
    "prebuild": "pnpm run generate:css",
    "prepack": "pnpm run build"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/themes": "^3.2.1",
    "cmdk": "^1.1.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-imask": "^7.6.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@ladle/react": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
